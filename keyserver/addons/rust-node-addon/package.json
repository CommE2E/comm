{
  "workspaces": {
    "nohoist": [
      "@napi-rs/cli"
    ]
  },
  "private": true,
  "name": "rust-node-addon",
  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "napi": {
    "name": "rust-node-addon",
    "triples": {
      "defaults": false,
      "additional": [
        "x86_64-apple-darwin",
        "aarch64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "aarch64-unknown-linux-gnu"
      ]
    }
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@napi-rs/cli": "^2.13.0"
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "yarn install-external-deps && napi build --platform napi --release",
    "build:debug": "napi build --platform napi",
    "version": "napi version",
    "postinstall": "yarn build",
    "clean": "rm -rf target/ && rm -rf napi/ && rm -rf node_modules/",
    "install-external-deps": "bash ./install_external_deps.sh"
  }
}