    "wagmi": "^2.5.18"
  },
  "jest": {
    "roots": [
      "<rootDir>"
    ],
    "transform": {
      "\\.js$": [
        "babel-jest",
        {
          "rootMode": "upward"
        }
      ]
    },
    "transformIgnorePatterns": [
      "/node_modules/(?!(@babel/runtime|thumbhash))"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "testEnvironment": "jsdom"
  }
